<!DOCTYPE rdf:RDF [
    <!ENTITY tile "http://localhost:8888/TILE/tile.owl#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
 ]>

<rdf:RDF 
    xmlns     ="&tile;" 
    xmlns:owl ="&owl;"
    xmlns:rdf ="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:xsd ="&xsd;">
	    <!-- information about this ontology -->
    <owl:Ontology rdf:about="">
      <rdfs:comment>Word Ontology</rdfs:comment>
      <rdfs:label>Text Image Linking Environment Word Ontology</rdfs:label>
    </owl:Ontology>
	<!-- a class, implicitly a subclass of owl:Thing -->
    <owl:Class rdf:ID="Word">
        <rdfs:subClassOf rdf:resource="&tile;Tag" /> 

		
	</owl:Class>
	</rdf:RDF>